<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.mepass">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/icon"
        android:label="MePass"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyLogin"
        tools:targetApi="31">
        <activity
            android:name="com.example.mepass.EditActivity"
            android:exported="false" />
        <activity
            android:name="com.example.mepass.Register"
            android:exported="false" />
        <activity
            android:name="com.example.mepass.NewEntry"
            android:exported="false" />
        <activity
            android:name="com.example.mepass.HomeActivity"
            android:exported="false" />
        <activity
            android:name="com.example.mepass.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


        </activity>
    </application>

</manifest>